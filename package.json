{
  "name": "rails-i18n-text-extractor",
  "displayName": "rails-i18n-text-extractor",
  "description": "Rails I18n Extractor is a VSCode extension designed to assist in the internationalization of Ruby on Rails projects.",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.87.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kb714/vscode-rails-i18n-text-extractor"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.extract",
        "title": "Extract"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "extension.extract",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.34",
    "@types/sinon": "^17.0.3",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.26.0",
    "glob": "^7.1.4",
    "minimatch": "5.1.2",
    "mocha": "^10.2.0",
    "sinon": "^17.0.1",
    "source-map-support": "^0.5.12",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "yaml": "^2.4.0"
  }
}
